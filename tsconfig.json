{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["dom", "DOM.Iterable", "esnext"],
		"removeComments": false,
		"declaration": true,
		"sourceMap": false,
		"strict": true,
		"noImplicitAny": false,
		"baseUrl": ".",
		"outDir": "dist",
		"declarationDir": ".",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true
	},
	"include": ["src"],
	"exclude": ["dist"]
}
